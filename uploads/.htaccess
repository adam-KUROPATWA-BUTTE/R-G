# Empêche juste l’indexation et l’exécution de scripts PHP
Options -Indexes

<IfModule mod_authz_core.c>
  <FilesMatch "\.(php|php\d*|phtml|phar)$">
    Require all denied
  </FilesMatch>
</IfModule>
<IfModule !mod_authz_core.c>
  <FilesMatch "\.(php|php\d*|phtml|phar)$">
    Deny from all
  </FilesMatch>
</IfModule>